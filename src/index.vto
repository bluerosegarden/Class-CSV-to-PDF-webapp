<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Class CSV to PDF</title>
  <!-- Loads the full bundle or the lite version from jsdelivr -->
  <!-- src="https://cdn.jsdelivr.net/npm/@myriaddreamin/typst-all-in-one.ts@0.6.1-rc3/dist/esm/index.js" -->
  <script type="module"
    src="https://cdn.jsdelivr.net/npm/@myriaddreamin/typst.ts/dist/esm/contrib/all-in-one-lite.bundle.js"
    id="typst"></script>
<link
  href="https://unpkg.com/normalize.css@8.0.1/normalize.css"
  rel="stylesheet"
/>
 <link rel="stylesheet" href="/style.css">

</head>

  <body>
    <h1 class="text-red-100">Johnson County Community College Class Formatter</h1>
 <!-- Provides a button to export to PDF after finish editing -->
  <!-- Accepts user input using a text area element -->
    <div>
      <a href="" id="bookmarklet">Class Grabber Bookmarklet</a></div>
  <div>

    <input type="file" id="file-input" multiple />
    <button id="preview-button" class="p-2 rounded-full bg-red-200">preview</button>
    <input type="checkbox" id="groupedCheckbox" name="groupedCheckbox">
    <label for="groupedCheckbox">Group by instructor</label>
  </div>
  <div>
    <button id="export" class="">Export To PDF</button>
  </div>
  <div id="content"></div>

  <script
    src="./js/script.js"
    id="script"></script>

  </body>


</html>
